<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="1100.0" spacing="10" style="-fx-background-color: #EDB175;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BusinessLogic.Controllers.VerificacionCodigoController">
    <children>
      <Pane prefHeight="589.0" prefWidth="1060.0">
         <children>
              <Label layoutX="359.0" layoutY="245.0" text="Ingrese el código de verificación:">
               <font>
                  <Font name="Arial Black" size="16.0" />
               </font>
            </Label>
              <TextField fx:id="txtCodigoVerificacion" layoutX="209.0" layoutY="279.0" prefHeight="26.0" prefWidth="589.0" />
              <Button fx:id="btnVerificarCodigo" layoutX="439.0" layoutY="316.0" onAction="#verificarCodigo" style="-fx-background-color: #FCFEFD;" text="Verificar Código">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Button>
            <Button fx:id="regresar" layoutX="964.0" layoutY="11.0" mnemonicParsing="false" onAction="#regresarLongIn" style="-fx-background-color: #FCFEFD;" text="Regresar">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Button>
              <Label fx:id="lblIntentosRestantes" layoutX="500.0" layoutY="364.0" prefHeight="18.0" prefWidth="16.0" />
         </children>
      </Pane>
    </children>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
